{
  "name": "woodcutter",
  "version": "0.1.2",
  "description": "A log module based on winston",
  "main": "dist/index.js",
  "repository": "https://github.com/oschweitzer/woodcutter.git",
  "typings": "definitions/index",
  "typescript": {
    "definition": "definitions/index"
  },
  "scripts": {
    "build": "tsc",
    "link:build": "tsc && npm link",
    "test": "mocha spec/**/*.spec.ts --require ts-node/register --reporter spec",
    "start": "node index.js",
    "watch": "tsc -w",
    "serve": "nodemon -e ts --watch src --exec \"npm run build && npm run start\" ",
    "release": "standard-version"
  },
  "keywords": [
    "log",
    "typescript",
    "winston"
  ],
  "author": "Olivier Schweitzer",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.1",
    "@types/mocha": "^2.2.46",
    "@types/moment": "^2.13.0",
    "@types/node": "^9.3.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "@types/winston": "^2.3.7",
    "chai": "^4.1.2",
    "mocha": "^5.0.0",
    "moment": "^2.20.1",
    "winston": "^2.4.0"
  }
}
